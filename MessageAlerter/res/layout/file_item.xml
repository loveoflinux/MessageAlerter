<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:id="@+id/file_item"
android:layout_width="wrap_content"
android:layout_height="32px"
android:orientation="horizontal">

	<LinearLayout android:id="@+id/nones"
	android:layout_width="wrap_content"
	android:layout_height="32px"
	android:orientation="horizontal"/> 

	<ImageView android:id="@+id/file_item_branch"
	android:layout_width="32px"
	android:layout_height="32px"/>
	
	<ImageView android:id="@+id/file_item_icon"
	android:layout_width="32px"
	android:layout_height="32px"/>
	
	<TextView android:id="@+id/file_item_name"
	android:layout_width="wrap_content"
	android:layout_height="32px"
	android:paddingRight="10px"
	android:gravity="center_vertical"
	android:textStyle="bold"/>
	
	<ImageView android:id="@+id/file_item_selected"
	android:layout_width="32px"
	android:layout_height="32px"/>

</LinearLayout>
